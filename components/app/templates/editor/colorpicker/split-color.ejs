<svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%">
  <rect x="0" y="0" rx="12.5%" ry="12.5%" width="100%" height="100%" fill="#fff"/>

  <svg x="2.5%" y="2.5%" width="95%" height="88.37209%">
      <clipPath id="round-corners">
          <rect x="0" y="0" width="100%" height="100%" rx="10%" ry="10%"/>
      </clipPath>

      <g clip-path="url(#round-corners)">
          <% for (var index in modelColors) { %>
              <rect x="0"
                    y="<%= (index * 100 / modelColors.length) - 3 %>%"
                    width="100%"
                    height="<%= (100 / modelColors.length) + 3 %>%"
                    fill="<%= modelColors[index] %>" />
          <% } %>
      </g>
  </svg>
</svg>
