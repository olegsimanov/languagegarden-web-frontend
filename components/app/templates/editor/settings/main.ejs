<div class="row-fluid">
    <div class="span3">
        <ul id="id_settings_nav" class="nav nav-tabs nav-stacked">
            <% _.each(view.panelDataOrdered, function (panelData) { %>
                <li>
                    <a class="nav-item <%= panelData.view.panelNavClass %>"
                       id="<%= panelData.view.cid %>" href="#">
                        <%= panelData.view.menuName %>
